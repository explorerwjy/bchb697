onsDeliveryExists": false, "oldFormatExists": false, "prenatalManifestationsAmnioticFluidExists": false, "cardiovascularVascularExists": false, "genitourinaryKidneysExists": false, "genitourinaryInternalGenitaliaMaleExists": false, "abdomenLiverExists": false, "chestExists": false, "headAndNeckTeethExists": false, "metabolicFeaturesExists": false, "laboratoryAbnormalities": "Cells show defective transcription-coupled nucleotide excision repair (TC-NER) after UV irradiation {UMLS:C3551174};\nIncreased cellular sensitivity to UV light {UMLS:C1857707} {HPO HP:0003224};\nNo increased sensitivity to oxidative species {UMLS:C3553299}", "skeletalPelvisExists": false, "growthExists": false, "abdomenGastrointestinalExists": false, "voiceExists": false, "skeletalExists": false, "skeletalLimbsExists": false, "laboratoryAbnormalitiesExists": true, "miscellaneousExists": true, "editHistory": "joanna : 05/25/2012\nckniffin : 5/15/2012", "abdomenSpleenExists": false, "respiratoryAirwaysExists": false, "headAndNeckExists": false, "neurologicExists": false, "abdomenExists": false, "skinNailsHairSkinElectronMicroscopyExists": false, "epochCreated": 1336978800, "immunologyExists": false, "skeletalFeetExists": false, "headAndNeckEarsExists": false, "respiratoryNasopharynxExists": false, "neurologicPeripheralNervousSystemExists": false, "neoplasiaExists": false, "chestBreastsExists": false, "abdomenExternalFeaturesExists": false, "respiratoryLarynxExists": false, "skinNailsHairSkin": "Photosensitivity {UMLS:C2362621} {HPO HP:0000992 UMLS:C0349506};\nFreckling {SNOMEDCT:699225003,403536009} {ICD10CM:L81.2} {UMLS:C0016689} {HPO HP:0001480 UMLS:C0016689}", "skinNailsHairExists": true, "chestDiaphragmExists": false, "prenatalManifestationsMaternalExists": false, "genitourinaryExternalGenitaliaFemaleExists": false, "inheritance": "Autosomal recessive {SNOMEDCT:258211005} {UMLS:C0441748} {HPO HP:0000007}", "growthOtherExists": false, "headAndNeckNeckExists": false, "skinNailsHairNailsExists": false, "neurologicCentralNervousSystemExists": false, "genitourinaryExternalGenitaliaMaleExists": false, "miscellaneous": "Onset in infancy {UMLS:C1848924} {HPO HP:0003593};\nNo predisposition to skin tumor development {UMLS:C3551176};\nOne patient has been reported (last curated May 2012) {UMLS:C3553301}", "respiratoryLungExists": false, "headAndNeckFaceExists": false, "skinNailsHairSkinExists": true, "epochUpdated": 1337929200, "headAndNeckNoseExists": false, "endocrineFeaturesExists": false, "chestExternalFeaturesExists": false, "muscleSoftTissueExists": false}, "prefix": "#", "titles": {"preferredTitle": "UV-SENSITIVE SYNDROME 2; UVSS2"}, "phenotypeMapExists": true, "textSectionList": [{"textSection": {"textSectionTitle": "Text", "textSectionContent": "A number sign (#) is used with this entry because UV-sensitive syndrome-2 (UVSS2) can be caused by homozygous mutation in the ERCC8 gene ({609412}) on chromosome 5.", "textSectionName": "text"}}, {"textSection": {"textSectionTitle": "Description", "textSectionContent": "UV-sensitive syndrome-2 is an autosomal recessive disorder characterized by cutaneous photosensitivity and increased freckling, without an increased risk of skin tumors. Patient cells show impaired recovery of RNA synthesis (RRS) after UV irradiation due to defective preferential repair of DNA damage in actively transcribing genes, although unscheduled DNA repair is normal. The cellular findings are consistent with a defect in transcription-coupled nucleotide excision repair (TC-NER) of UV damage (summary by {1:Nardo et al., 2009}).\n\nSee also Cockayne syndrome type A (CSA; {216400}), an allelic disorder with a more severe phenotype including neurologic symptoms and skeletal abnormalities.\n\nFor a general phenotypic description and a discussion of genetic heterogeneity of UVSS, see UVSS1 ({600630}).", "textSectionName": "description"}}, {"textSection": {"textSectionTitle": "Clinical Features", "textSectionContent": "{1:Nardo et al. (2009)} reported a 15-year-old French girl with UV-sensitive syndrome. She presented at age 4 months with sun sensitivity manifest as easy sun burning and erythema. She had numerous freckles on her face and exposed areas of the neck, but no history or evidence of cutaneous tumors. Psychomotor development was normal. Patient-derived fibroblasts showed a reduced recovery of RNA synthesis after UV irradiation and a defective capacity to repair UV-induced damage on the transcribed strand of active genes, indicating a defect in transcription-coupled nucleotide excision repair (TC-NER). However, there was no hypersensitivity to reactive oxygen species, and UV-induced DNA repair synthesis and global genome NER (GG-NER) were normal.", "textSectionName": "clinicalFeatures"}}, {"textSection": {"textSectionTitle": "Inheritance", "textSectionContent": "The transmission pattern of UVSS2 in the patient reported by {1:Nardo et al. (2009)} was consistent with autosomal recessive inheritance.", "textSectionName": "inheritance"}}, {"textSection": {"textSectionTitle": "Molecular Genetics", "textSectionContent": "In a girl with UVSS2, {1:Nardo et al. (2009)} identified a homozygous mutation in the ERCC8 gene (W361C; {609412.0006}). {1:Nardo et al. (2009)} hypothesized that the mild phenotype in this patient was due to the lack of cellular sensitivity to oxidative stress.", "textSectionName": "molecularGenetics"}}], "editHistory": "joanna : 05/31/2012\njoanna : 5/31/2012\ncarol : 5/31/2012\njoanna : 5/30/2012\njoanna : 5/30/2012\ncarol : 5/16/2012\nckniffin : 5/15/2012", "phenotypicSeriesExists": true, "creationDate": "Cassandra L. Kniffin : 5/14/2012", "epochUpdated": 1338447600, "dateUpdated": "Thu, 31 May 2012 03:00:00 EDT", "referenceList": [{"reference": {"articleUrl": "http://www.pnas.org/cgi/pmidlookup?view=long&pmid=19329487", "publisherName": "HighWire Press", "title": "A UV-sensitive syndrome patient with a specific CSA mutation reveals separable roles for CSA in response to UV and oxidative DNA damage.", "mimNumber": 614621, "referenceNumber": 1, "publisherAbbreviation": "HighWire", "pubmedID": 19329487, "source": "Proc. Nat. Acad. Sci. 106: 6209-6214, 2009.", "authors": "Nardo, T., Oneda, R., Spivak, G., Vaz, B., Mortier, L., Thomas, P., Orioli, D., Laugel, V., Stary, A., Hanawalt, P. C., Sarasin, A., Stefanini, M.", "pubmedImages": true, "pubmedCentralID": 2667150, "publisherUrl": "http://highwire.stanford.edu"}}], "externalLinks": {"gwasCatalog": false, "wormbaseDO": false, "mgiHumanDisease": false, "orphanetDiseases": "178338;;18016;;UV-sensitive syndrome", "dermAtlas": false, "swissProtIDs": "Q13216", "umlsIDs": "C3553298", "keggPathways": false, "gtr": true, "cmgGene": false, "nextGxDx": false, "geneTests": false, "geneticAllianceIDs": "9442", "clinGen": false, "geneticsHomeReferenceIDs": "condition;;UV-sensitive syndrome;;uv-sensitive-syndrome", "diseaseOntologyIDs": "0060240"}}